{
  "name": "vector-store-app",
  "version": "1.0.0",
  "description": "Dynamic vector store management with ChatGPT-like interface",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": ["vector-store", "document-processing", "embeddings", "chatgpt"],
  "author": "mcli",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.6.4"
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "fs-extra": "^11.1.1",
    "path": "^0.12.7",
    "uuid": "^9.0.1",
    "python-shell": "^5.0.0",
    "node-fetch": "^3.3.2",
    "ws": "^8.14.2",
    "sqlite3": "^5.1.6",
    "better-sqlite3": "^9.2.2",
    "sharp": "^0.33.0",
    "pdf-parse": "^1.1.1",
    "mammoth": "^1.6.0",
    "csv-parser": "^3.0.0",
    "xml2js": "^0.6.2",
    "cheerio": "^1.0.0-rc.12",
    "natural": "^6.10.4",
    "stopword": "^2.0.8",
    "compression": "^1.7.4",
    "helmet": "^7.1.0",
    "rate-limiter-flexible": "^3.0.8"
  },
  "build": {
    "appId": "com.mcli.vectorstore",
    "productName": "Vector Store Manager",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!node_modules/**/*",
      "!dist/**/*",
      "!build/**/*"
    ],
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
} 